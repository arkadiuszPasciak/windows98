    "lintfix:prettier": "prettier --write --list-different .",
    "lint:markup": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10"
