{
  "name": "windows98",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier .  --write",
    "lint:style": "npm run lint:style:vue & npm run lint:style:scss",
    "lint:style:vue": "stylelint src/**/*.vue --custom-syntax postcss-html",
    "lint:style:scss": "stylelint src/**/*.scss --custom-syntax postcss-scss",
    "lint:all": "npm run lint:eslint & npm run lint:prettier & npm run lint:style",
    "lint:markup": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "cypress:open": "cypress open",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "pinia": "^2.0.14",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@pinia/testing": "^0.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitest/ui": "^0.17.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "commitlint-config-jira": "^1.6.4",
    "commitlint-plugin-jira-rules": "^1.6.4",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.17.0",
    "vue-tsc": "^0.35"
  }
}
