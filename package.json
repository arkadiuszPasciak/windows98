    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.17.0",
    "vue-tsc": "^0.35"
